<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FriendListActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="81dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="82dp"
        android:text="Friends List"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fl_home_button" />

    <ImageButton
        android:id="@+id/fl_home_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/house" />

    <ImageButton
        android:id="@+id/fl_setting_button"
        android:layout_width="77dp"
        android:layout_height="81dp"
        android:layout_marginStart="225dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="6dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/fl_home_button"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gear" />

    <Button
        android:id="@+id/tim_button"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="106dp"
        android:backgroundTint="@color/white"
        android:text="Tim"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/trey_button"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="106dp"
        android:backgroundTint="@color/white"
        android:text="Trey"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tim_button" />

    <Button
        android:id="@+id/alex_button"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="188dp"
        android:backgroundTint="@color/white"
        android:text="Alex"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="46dp"
        android:layout_height="38dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/tim_button"
        app:layout_constraintEnd_toEndOf="@+id/tim_button"
        app:layout_constraintTop_toTopOf="@+id/tim_button"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="47dp"
        android:layout_height="43dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/trey_button"
        app:layout_constraintEnd_toEndOf="@+id/trey_button"
        app:layout_constraintStart_toStartOf="@+id/trey_button"
        app:layout_constraintTop_toTopOf="@+id/trey_button"
        app:srcCompat="@drawable/gery" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toBottomOf="@+id/alex_button"
        app:layout_constraintEnd_toEndOf="@+id/alex_button"
        app:layout_constraintStart_toStartOf="@+id/alex_button"
        app:layout_constraintTop_toTopOf="@+id/alex_button"
        app:srcCompat="@drawable/red" />
</androidx.constraintlayout.widget.ConstraintLayout>